{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "__Domain__",
    "TenantId": "__TenantId__",
    "ClientId": "__ClientId__",
    "CallbackPath": "/signin-oidc"
  },
  "SecuritySettings": {
    "HttpsCertificateThumbprint": "__CertificateThumbprint__"
  },
  "Swagger": {
    "ClientId": "__SwaggerClientId__",
    "ClientSecret": "__SwaggerSecret__",
    "Realm": "https://__Relam__/swagger/ui/o2c-html",
    "AppName": "__AppName__",
    "Audience": "__Audience__"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
